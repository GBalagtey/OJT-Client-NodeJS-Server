<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= pageTitle %></title>
</head>
<body>

  <h1><%= welcomeMessage %></h1>

  <% sections.forEach((section) => { %>
    <section>
      <h2><%= section.title %></h2>
      <p><%= section.content %></p>
    </section>
  <% }); %>

</body>
</html>
